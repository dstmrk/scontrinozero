{
  "openapi": "3.0.1",
  "info": {
    "title": "Scontrino Rapido API",
    "description": "API for massive sending of Documenti Commerciali to the Agenzia delle Entrate.",
    "termsOfService": "https://scontrinorapido.it",
    "contact": {
      "name": "Claudio Pizzillo",
      "url": "https://www.linkedin.com/in/claudio-pizzillo-7a232924",
      "email": "claudio@kefa.it"
    },
    "license": {
      "name": "Scontrino Rapido API License.",
      "url": "https://scontrinorapido.it"
    },
    "version": "v1"
  },
  "paths": {
    "/api/DC/GetJsonDC": {
      "get": {
        "tags": [
          "DC"
        ],
        "summary": "Get Documento Commerciale json by trasmission id (idtrx).",
        "parameters": [
          {
            "name": "piva",
            "in": "header",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "token",
            "in": "header",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "idtrx",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "usr",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "pwd",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "pin",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "piva",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "tipoincarico",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "sceltapiva",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Returns the Documento Commerciale json",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RootObject"
                }
              }
            }
          },
          "400": {
            "description": "If the idtrx is not found or if you are unable to authenticate with Agenzia delle Entrate"
          },
          "401": {
            "description": "You are unauthenticaed to use the API. Check your headers!"
          }
        }
      }
    },
    "/api/DC/GetPdfDC": {
      "get": {
        "tags": [
          "DC"
        ],
        "summary": "Get Documento Commerciale pdf by trasmission id (idtrx).",
        "parameters": [
          {
            "name": "piva",
            "in": "header",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "token",
            "in": "header",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "idtrx",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "usr",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "pwd",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "pin",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "piva",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "tipoincarico",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "sceltapiva",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Returns the Documento Commerciale pdf from Agenzia delle Entrate",
            "content": {
              "application/pdf": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          },
          "400": {
            "description": "If the idtrx is not found or if you are unable to authenticate with Agenzia delle Entrate"
          },
          "401": {
            "description": "You are unauthenticaed to use the API. Check your headers!"
          }
        }
      }
    },
    "/api/DC/SendDCs": {
      "post": {
        "tags": [
          "DC"
        ],
        "summary": "Send an json array of Documento Commerciale.",
        "parameters": [
          {
            "name": "piva",
            "in": "header",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "token",
            "in": "header",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "usr",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "pwd",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "pin",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "piva",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "tipoincarico",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "sceltapiva",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/RootObject"
                }
              }
            },
            "text/json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/RootObject"
                }
              }
            },
            "application/*+json": {
              "schema": {
                "type": "array",
                "items": {
                  "$ref": "#/components/schemas/RootObject"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Returns the list of esiti for each documento commerciale sent in json",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Esito"
                }
              }
            }
          },
          "400": {
            "description": "If you are unable to authenticate with Agenzia delle Entrate"
          },
          "401": {
            "description": "You are unauthenticaed to use the API. Check your headers!"
          }
        }
      }
    },
    "/api/DC/SendAnnullo": {
      "post": {
        "tags": [
          "DC"
        ],
        "summary": "Send an Annullo for a documento commerciale by trasmission id (idtrx) and ProgressivoTrasmissione.",
        "parameters": [
          {
            "name": "piva",
            "in": "header",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "token",
            "in": "header",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "idtrx",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "ProgressivoTrasmissione",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "usr",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "pwd",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "pin",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "piva",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "tipoincarico",
            "in": "query",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "sceltapiva",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Returns the list of esiti for each documento commerciale sent in json",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Esito"
                }
              }
            }
          },
          "400": {
            "description": "If you are unable to authenticate with Agenzia delle Entrate"
          },
          "401": {
            "description": "You are unauthenticaed to use the API. Check your headers!"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "AltriDatiIdentificativi": {
        "type": "object",
        "properties": {
          "denominazione": {
            "type": "string",
            "nullable": true
          },
          "indirizzo": {
            "type": "string",
            "nullable": true
          },
          "numeroCivico": {
            "type": "string",
            "nullable": true
          },
          "cap": {
            "type": "string",
            "nullable": true
          },
          "comune": {
            "type": "string",
            "nullable": true
          },
          "provincia": {
            "type": "string",
            "nullable": true
          },
          "nazione": {
            "type": "string",
            "default": "IT",
            "nullable": true
          },
          "modificati": {
            "type": "boolean",
            "default": "false"
          },
          "defAliquotaIVA": {
            "type": "string",
            "nullable": true
          },
          "nuovoUtente": {
            "type": "boolean",
            "default": "false"
          }
        },
        "additionalProperties": false
      },
      "CedentePrestatore": {
        "type": "object",
        "properties": {
          "identificativiFiscali": {
            "$ref": "#/components/schemas/IdentificativiFiscali"
          },
          "altriDatiIdentificativi": {
            "$ref": "#/components/schemas/AltriDatiIdentificativi"
          }
        },
        "additionalProperties": false
      },
      "DatiTrasmissione": {
        "type": "object",
        "properties": {
          "formato": {
            "type": "string",
            "default": "DCW10",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "DocumentoCommerciale": {
        "type": "object",
        "properties": {
          "flagDocCommPerRegalo": {
            "type": "boolean"
          },
          "progressivoCollegato": {
            "type": "string",
            "nullable": true
          },
          "dataOra": {
            "type": "string",
            "nullable": true
          },
          "importoTotaleIva": {
            "type": "number",
            "format": "float"
          },
          "scontoTotale": {
            "type": "number",
            "format": "double"
          },
          "scontoTotaleLordo": {
            "type": "number",
            "format": "double"
          },
          "totaleImponibile": {
            "type": "number",
            "format": "double"
          },
          "ammontareComplessivo": {
            "type": "number",
            "format": "double"
          },
          "totaleNonRiscosso": {
            "type": "number",
            "format": "double"
          },
          "elementiContabili": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ElementiContabili"
            },
            "nullable": true
          },
          "vendita": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Vendita"
            },
            "nullable": true
          },
          "scontoAbbuono": {
            "type": "number",
            "format": "double"
          },
          "importoDetraibileDeducibile": {
            "type": "number",
            "format": "double"
          },
          "resoAnnullo": {
            "$ref": "#/components/schemas/ResoAnnullo"
          },
          "numeroProgressivo": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ElementiContabili": {
        "type": "object",
        "properties": {
          "idElementoContabile": {
            "type": "string",
            "nullable": true
          },
          "resiPregressi": {
            "type": "number",
            "format": "double"
          },
          "reso": {
            "type": "number",
            "format": "double"
          },
          "quantita": {
            "type": "number",
            "format": "double"
          },
          "descrizioneProdotto": {
            "type": "string",
            "nullable": true
          },
          "prezzoLordo": {
            "type": "number",
            "format": "double"
          },
          "prezzoUnitario": {
            "type": "number",
            "format": "double"
          },
          "scontoUnitario": {
            "type": "number",
            "format": "double"
          },
          "scontoLordo": {
            "type": "number",
            "format": "double"
          },
          "aliquotaIVA": {
            "type": "string",
            "nullable": true
          },
          "importoIVA": {
            "type": "number",
            "format": "double"
          },
          "imponibile": {
            "type": "number",
            "format": "double"
          },
          "imponibileNetto": {
            "type": "number",
            "format": "double"
          },
          "totale": {
            "type": "number",
            "format": "double"
          },
          "omaggio": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Errore": {
        "type": "object",
        "properties": {
          "codice": {
            "type": "string",
            "nullable": true
          },
          "descrizione": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Esito": {
        "type": "object",
        "properties": {
          "esito": {
            "type": "boolean"
          },
          "idtrx": {
            "type": "string",
            "nullable": true
          },
          "progressivo": {
            "type": "string",
            "nullable": true
          },
          "errori": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Errore"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "IdentificativiFiscali": {
        "type": "object",
        "properties": {
          "codicePaese": {
            "type": "string",
            "default": "IT",
            "nullable": true
          },
          "partitaIva": {
            "type": "string",
            "nullable": true
          },
          "codiceFiscale": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "ResoAnnullo": {
        "type": "object",
        "properties": {
          "tipologia": {
            "type": "string",
            "nullable": true
          },
          "dataOra": {
            "type": "string",
            "nullable": true
          },
          "progressivo": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RootObject": {
        "type": "object",
        "properties": {
          "datiTrasmissione": {
            "$ref": "#/components/schemas/DatiTrasmissione"
          },
          "cedentePrestatore": {
            "$ref": "#/components/schemas/CedentePrestatore"
          },
          "documentoCommerciale": {
            "$ref": "#/components/schemas/DocumentoCommerciale"
          },
          "flagIdentificativiModificati": {
            "type": "boolean"
          },
          "idtrx": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Vendita": {
        "type": "object",
        "properties": {
          "tipo": {
            "type": "string",
            "nullable": true
          },
          "importo": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      }
    }
  }
}
